vector<vector<string>> groupAnagrams(vector<string>& strs) {
        // use map to compare
        vector<vector<string>> res;
        unordered_map<string, vector<string>> map;
        for(string s : strs){
            vector<int> cnt(26, 0);
            string t = "";
            for(char c : s)
                cnt[c - 'a'] += 1;
            for(int d : cnt)
                t += (to_string(d) + "/");
            map[t].push_back(s);
        }
        for(auto a : map){
            res.push_back(a.second);
        }
        return res;
    }
